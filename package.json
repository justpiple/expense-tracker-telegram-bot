{
  "name": "expense-tracker-bot",
  "version": "1.0.0",
  "description": "A Telegram bot that tracks expenses and saves them to Notion",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "start": "bun dist/index.js",
    "dev": "bun --watch src/index.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@notionhq/client": "^2.3.0",
    "dotenv": "^16.4.7",
    "node-cron": "^3.0.3",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "bun-types": "^1.2.8"
  }
}
